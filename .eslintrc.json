{
  "extends": [
    "next/core-web-vitals",
    "standard",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "comma-dangle": "off",
    "indent": "off",
    "space-in-parens": [
      "error", "always"
    ],
    "space-in-brackets": ["error", "always"],
    "template-curly-spacing": [
      "error", "always"
    ]
  }
}
