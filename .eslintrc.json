{
  "extends": [
    "next/core-web-vitals",
    "standard",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "comma-dangle": "off",
    "indent": "off",
    "space-in-parens": [
      "error",
      "always"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "array-bracket-spacing": [
      "error",
      "always"
    ],
    "template-curly-spacing": [
      "error",
      "always"
    ]
  }
}
